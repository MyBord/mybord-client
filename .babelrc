{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["babel-plugin-transform-builtin-extend", {
      "globals": ["Error", "Array"],
      "approximate": true
    }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "api": "./src/api/",
        "app": "./src/spa/app/",
        "assets": "./src/assets/",
        "buttons": "./src/spa/app/shared/buttons/",
        "gapi": "./src/thirdParty/gapi/",
        "header": "./src/spa/app/header/",
        "hooks": "./src/hooks/",
        "icons": "./src/spa/app/shared/icons/",
        "initializeApp": "./src/spa/app/initializeApp/",
        "layout": "./src/spa/layout/",
        "lotty": "./src/thirdParty/lotty/",
        "mockData": "./src/mockData/",
        "navigation": "./src/spa/layout/navigation/",
        "pages": "./src/spa/app/pages/",
        "routing": "./src/spa/app/routing/",
        "shared": "./src/spa/app/shared/",
        "static": "./src/static/",
        "storybook": "./src/thirdParty/storybook/",
        "styles": "./src/styles/",
        "typography": "./src/spa/app/shared/typography/",
        "types": "./src/types/",
        "utils": "./src/utils/"
      }
    }]
  ]
}

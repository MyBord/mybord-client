{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-transform-builtin-extend", {
      "globals": ["Error", "Array"],
      "approximate": true
    }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "alert": "./src/spa/components/atoms/alert/",
        "animations": "./src/spa/components/neutrons/animations/",
        "api": "./src/api/",
        "app": "./src/spa/app/",
        "atoms": "./src/spa/components/atoms/",
        "backgrounds": "./src/spa/components/neutrons/backgrounds/",
        "buttons": "./src/spa/components/atoms/buttons/",
        "copy": "./src/spa/components/molecules/copy/",
        "context": "./src/context/",
        "fallbacks": "./src/spa/components/atoms/fallbacks/",
        "footer": "./src/spa/components/organisms/footer/",
        "form": "./src/spa/components/atoms/form/",
        "formItem": "./src/spa/components/atoms/formItem/",
        "forms": "./src/spa/components/molecules/forms/",
        "graphics": "./src/spa/components/neutrons/graphics/",
        "header": "./src/spa/app/layout/header/",
        "hooks": "./src/hooks/",
        "icon": "./src/spa/components/neutrons/icon/",
        "images": "./src/spa/components/neutrons/images/",
        "inputs": "./src/spa/components/atoms/inputs/",
        "landing": "./src/spa/landing/",
        "layout": "./src/spa/app/layout/",
        "lottiePlayer": "./src/spa/components/neutrons/lottiePlayer/",
        "lotties": "./src/spa/components/neutrons/lotties/",
        "menus": "./src/spa/components/molecules/menus/",
        "mockData": "./src/mockData/",
        "modal": "./src/spa/components/molecules/modal/",
        "modals": "./src/spa/components/organisms/modals/",
        "molecules": "./src/spa/components/molecules/",
        "navigation": "./src/spa/app/layout/navigation/",
        "neutrons": "./src/spa/components/neutrons/",
        "organisms": "./src/spa/components/organisms/",
        "pages": "./src/spa/app/pages/",
        "popOver": "./src/spa/components/atoms/popOver/",
        "portal": "./src/spa/components/neutrons/portal/",
        "routing": "./src/spa/app/routing/",
        "schema": "./src/server/schema/",
        "server": "./src/server/",
        "storybook": "./src/thirdParty/storybook/",
        "styles": "./src/styles/",
        "tags": "./src/spa/components/molecules/tags/",
        "templates": "./src/spa/components/templates/",
        "typography": "./src/spa/components/atoms/typography/",
        "types": "./src/types/",
        "userCard": "./src/spa/components/organisms/userCard/",
        "utils": "./src/utils/"
      }
    }]
  ]
}

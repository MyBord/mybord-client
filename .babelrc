{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-transform-builtin-extend", {
      "globals": ["Error", "Array"],
      "approximate": true
    }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "header": "./src/app/header/",
        "navigation": "./src/app/navigation/",
        "pages": "./src/app/pages/",
        "shared": "./src/app/shared/",
        "static": "./src/static/",
        "storybook": "./src/thirdParty/storybook/",
        "styles": "./src/styles/"
      }
    }]
  ]
}

{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "shared/*": ["src/app/shared/*"],
      "static/*": ["src/static/*"],
      "storybook/*": ["src/thirdParty/storybook/*"]
    }
  }
}
